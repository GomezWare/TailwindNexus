{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": [
        "jsons/*"
      ],
      "@services/*": [
        "src/libs/services/*"
      ],
      "@utils/*": [
        "src/libs/utils/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}